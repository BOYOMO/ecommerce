<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tocatalogue</from-outcome>
            <to-view-id>/catalogue.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>toshoppingCart</from-outcome>
            <to-view-id>/shoppingCart.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>toproduitcreation</from-outcome>
            <to-view-id>/produitcreation.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/catalogue.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromcatalogue</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/shoppingCart.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/shoppingCart.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromshoppingCart</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/produitcreation.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromproduitcreation</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fincreation</from-outcome>
            <to-view-id>/catalogue.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
